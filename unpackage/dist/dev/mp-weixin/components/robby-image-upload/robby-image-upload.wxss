
.imageUploadContainer {
	/* padding: 10upx 5upx; */
	/* margin: 10upx 5upx; */
}
.dragging {
	-webkit-transform: scale(1.2);
	        transform: scale(1.2)
}
.imageUploadList {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.imageItem,
.imageUpload {
	width: 100rpx;
	height: 100rpx;
	margin: 10rpx;
}
.imageDel {
	position: relative;
	left: 80rpx;
	bottom: 155rpx;
	background-color: rgba(0, 0, 0, 0.5);
	width: 30rpx;
	text-align: center;
	line-height: 30rpx;
	-webkit-border-radius: 15rpx;
	        border-radius: 15rpx;
	color: white;
	font-size: 25rpx;
	padding-bottom: 2rpx;
}
.imageItem image,
.moveImage {
	width: 100rpx;
	height: 100rpx;
	-webkit-border-radius: 8rpx;
	        border-radius: 8rpx;
}
.imageUpload {
	line-height: 100rpx;
	text-align: center;
	font-size: 60rpx;
	color: #D9D9D9;
	border: 1px solid #D9D9D9;
	-webkit-border-radius: 8rpx;
	        border-radius: 8rpx;
}
.moveImage {
	position: absolute;
}

